<VirtualHost *:80>
  DocumentRoot /var/www/sub-domains/com.web.nextcloud/html/
  ServerName  nextcloud.web.com

  <Directory /var/www/sub-domains/com.web.nextcloud/html/>
    Require all granted
    AllowOverride All
    Options FollowSymLinks MultiViews

    <IfModule mod_dav.c>
      Dav off
    </IfModule>
  </Directory>
</VirtualHost>
